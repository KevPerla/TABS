<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tablayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/primary">

        <com.google.android.material.tabs.TabItem
            android:id="@+id/tabquiensoy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quien soy?"
            android:icon="@drawable/icono_quiensoy"
            />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/tabque_estudio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Que estudio"
            android:icon="@drawable/baseline_school_24"
            />

        <com.google.android.material.tabs.TabItem
            android:id="@+id/tabtecnologia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tecnologias"
            android:icon="@drawable/icono_tecno"
            />
    </com.google.android.material.tabs.TabLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </androidx.viewpager.widget.ViewPager>
</LinearLayout>